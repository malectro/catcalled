<h1>Edit <%=@participant.name%>'s Exit Interview</h1>

<%= form_for([:admin, @participant, @exit_interview], url: admin_participant_exit_interview_path) do |f| %>
  <div><%= notice %></div>

  <%= f.text_area :text %>

  <div>
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', admin_participants_path, class: 'back' %>
